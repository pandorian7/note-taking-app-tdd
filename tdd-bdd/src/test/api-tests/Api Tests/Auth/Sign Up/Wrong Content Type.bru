meta {
  name: Wrong Content Type
  type: http
  seq: 4
}

post {
  url: {{baseURL}}/auth/signup
  body: multipartForm
  auth: inherit
}

body:json {
  {
    "username":"user123",
    "password":"Hello@123",
    "firstName": "First", 
    "lastName": "Last"
  }
}

tests {
  test("should return 415 for unsupported media type", function () {
    const status = res.getStatus();
    const body = res.getBody();
  
    expect(status).to.equal(415);
    expect(body.code).to.equal("UNSUPPORTED_MEDIA_TYPE");
  
    console.warn("Unsupported media type (warning).");
  });
  
}
